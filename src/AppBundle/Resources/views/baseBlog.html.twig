{% extends 'base.html.twig' %}

{% block header %}
    <div class="container">
        <div class="row">
            <div class="col-md-12">
                Header
            </div>
        </div>
    </div>
{% endblock %}
{% block body %}
    <div class="container">
        <div class="row">
            <div class="col-md-10 offset-1">
                {% block posts %}
                {% endblock %}
            </div>
        </div>
    </div>
{% endblock %}
{% block footer %}
    <div class="container">
        <div class="row">
            <div class="col-md-12">
                Footer
            </div>
        </div>
    </div>
{% endblock %}